  // Update phone validation and OTP handling
  function handlePhoneValidation() {
    console.log("handlePhoneValidation() called ");

    const phoneInput = document.querySelector("#applyFormWrapper #phone");
    const sendOtpBtn = document.querySelector("#applyFormWrapper #sendOtpBtn");
    const otpInputs = document.querySelectorAll(".otp-input");
    const timerDisplay = document.getElementById("timer");
    const resendBtn = document.getElementById("resendOtpBtn");
    const otpWrapper = document.querySelector(".resend-otp-wrapper");

    if (!phoneInput || !sendOtpBtn) {
      console.error("Phone input or send button not found ");
      return;
    }

    // Enable button immediately
    phoneInput.addEventListener("input", function () {
      if (this.value.trim().length === 10 && /^\d{10}$/.test(this.value)) {
        sendOtpBtn.disabled = false;
        sendOtpBtn.classList.add("active");
      } else {
        sendOtpBtn.disabled = true;
        sendOtpBtn.classList.remove("active");
      }
    });

    sendOtpBtn.addEventListener("click", async function () {
      const phone = phoneInput.value.trim();
      console.log("Sending OTP to:", phone);

      try {
        sendOtpBtn.textContent = "Sending...";
        sendOtpBtn.disabled = true;

        const response = await fetch(
          "https://api.cvmu.edu.in/applicant/api/v1/applicant_mobile_and_email/otp_send",
          {
            method: "PUT",
            headers: {
              "Content-Type": "application/json",
            },
            body: JSON.stringify({
              mobile_number: phone,
              country_code: "91" // Add country code for India
            }),
          }
        );

        const data = await response.json();
        console.log("API Response:", data);

        if (data.success === true) {
          sendOtpBtn.textContent = "Sent ";
          sendOtpBtn.classList.add("sent");

          //  Enable OTP inputs
          otpInputs.forEach((input) => {
            input.disabled = false;
            input.value = "";
          });
          otpInputs[0].focus();

          //  Show resend section
          otpWrapper.style.display = "flex";

          //  Start timer
          let timeLeft = 60;
          resendBtn.disabled = true;
          timerDisplay.textContent = `Wait ${timeLeft}s`;

          const timer = setInterval(() => {
            timeLeft--;
            timerDisplay.textContent = `Wait ${timeLeft}s`;
            if (timeLeft <= 0) {
              clearInterval(timer);
              timerDisplay.textContent = "";
              resendBtn.disabled = false;
            }
          }, 1000);
        } else {
          throw new Error(data.message || "Failed to send OTP");
        }
      } catch (err) {
        console.error("OTP API error:", err.message);
        sendOtpBtn.textContent = "Send OTP";
        sendOtpBtn.disabled = false;
      }
    });
  }



  async function verifyOTP() {
    const phone = document.querySelector("#applyFormWrapper #phone").value.trim();
    const otp = document.getElementById("otp").value.trim();
    const otpInputs = document.querySelectorAll(".otp-input");
    const sendOtpBtn = document.getElementById("sendOtpBtn");
    const phoneInput = document.getElementById("phone");
    const resendWrapper = document.querySelector(".resend-otp-wrapper");

    if (otp.length !== 6) {
      alert("Please enter a valid 6-digit OTP");
      return;
    }

    try {
      const response = await fetch("https://api.cvmu.edu.in/applicant/api/v1/applicant_mobile_and_email/otp_verify", {
        method: "PUT",
        headers: {
          "Content-Type": "application/json",
        },
        body: JSON.stringify({
          mobile_number: phone,
          otp: otp
        })
      });

      const data = await response.json();
      console.log("Verify OTP Response:", data);

      if (data.success) {
        // Disable OTP inputs
        otpInputs.forEach(input => {
          input.disabled = true;
          input.style.borderColor = "#28a745";
        });

        // Disable phone input
        phoneInput.disabled = true;

        // Hide Send OTP button
        if (sendOtpBtn) {
          sendOtpBtn.style.display = "none";
        }

        // Hide resend section
        if (resendWrapper) {
          resendWrapper.style.display = "none";
        }

        // Stop timer
        if (window.otpTimer) {
          clearInterval(window.otpTimer);
        }

        // Show verified badge
        const verifiedBadge = document.createElement("span");
        verifiedBadge.textContent = " Verified";
        verifiedBadge.style.color = "#28a745";
        verifiedBadge.style.marginLeft = "10px";
        verifiedBadge.style.fontWeight = "bold";
        verifiedBadge.style.fontSize = "14px";
        verifiedBadge.style.marginTop = "5px";
        verifiedBadge.style.display = "inline-block";

        phoneInput.parentElement.appendChild(verifiedBadge);
        return true;
      } else {
        alert(" Invalid OTP, please try again.");
        return false;
      }
    } catch (err) {
      console.error("Error verifying OTP:", err);
      alert("Something went wrong. Please try again.");
      return false;
    }
  }


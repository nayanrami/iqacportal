  document.addEventListener("DOMContentLoaded", () => {
    const resendBtn = document.getElementById("resendOtpBtn");
    if (resendBtn) {
      // Remove existing listeners
      const newResendBtn = resendBtn.cloneNode(true);
      resendBtn.parentNode.replaceChild(newResendBtn, resendBtn);

      // Add single click handler
      newResendBtn.addEventListener("click", () => {
        const sendOtpBtn = document.getElementById("sendOtpBtn");
        if (sendOtpBtn && !window.isVerifyingOTP) {
          sendOtpBtn.click();
        }
      });
    }
  });

  // Function to fetch and populate eligibility options
  async function populateEligibilityDropdown() {
    const eligibilitySelect = document.getElementById("eligibility");

    if (!eligibilitySelect) {
      console.error("Eligibility select element not found");
      return;
    }

    try {
      const response = await fetch("https://api.cvmu.edu.in/university/api/v1/eligibility_head/view");
      const data = await response.json();

      if (data.success) {
        eligibilitySelect.innerHTML = '<option value="">Select your eligibility</option>';

        data.data.results.forEach(eligibility => {
          const option = document.createElement("option");
          option.value = eligibility.id;
          option.textContent = eligibility.name;
          eligibilitySelect.appendChild(option);
        });

        // Remove any existing event listeners
        const newEligibilitySelect = eligibilitySelect.cloneNode(true);
        eligibilitySelect.parentNode.replaceChild(newEligibilitySelect, eligibilitySelect);

        // Add change event listener
        newEligibilitySelect.addEventListener("change", async function () {
          const selectedEligibilityId = this.value;
          console.log("Eligibility changed to:", selectedEligibilityId);

          const programSelect = document.getElementById("program");
          if (!programSelect) {
            console.error("Program select element not found");
            return;
          }

          if (selectedEligibilityId) {
            programSelect.innerHTML = '<option value="">Loading programs...</option>';
            try {
              const response = await fetch(`https://api.cvmu.edu.in/applicant/api/v1/applicant_portal/get_programs?status=Open&eligibility_head_id=${selectedEligibilityId}`);
              const data = await response.json();

              if (data.success) {
                populateProgramDropdown(data.data.results);
              } else {
                programSelect.innerHTML = '<option value="">Error loading programs</option>';
              }
            } catch (error) {
              console.error("Error fetching programs:", error);
              programSelect.innerHTML = '<option value="">Error loading programs</option>';
            }
          } else {
            programSelect.innerHTML = '<option value="">First select eligibility</option>';
          }
        });
      }
    } catch (error) {
      console.error("Error fetching eligibility data:", error);
      newEligibilitySelect.innerHTML = '<option value="">Error loading eligibility data</option>';
    }
  }

  // Update populateProgramDropdown to use correct ID
  function populateProgramDropdown(programs) {
    const select = document.getElementById("program");
    if (!select) {
      console.error("Program select element not found in populateProgramDropdown");
      return;
    }

    select.innerHTML = '<option value="">Select Program</option>';

    if (programs && Array.isArray(programs)) {
      programs.forEach((program) => {
        const option = document.createElement("option");
        option.value = program.program_id;
        option.textContent = program.program_data.name;
        option.dataset.admissionId = program.admission_id;
        option.dataset.admissionName = program.admission_name;
        option.dataset.startDate = program.start_date;
        option.dataset.endDate = program.end_date;
        option.dataset.totalIntake = program.total_intake;
        option.dataset.programTypeId = program.program_data.program_type_id;
        select.appendChild(option);
      });
    }
  }

  // Initialize the form when document is ready
  document.addEventListener("DOMContentLoaded", function () {
    // Fetch eligibility data
    populateEligibilityDropdown();

    // Add change event listener for eligibility dropdown
    document.getElementById("eligibility")?.addEventListener("change", function () {
      const selectedEligibilityId = this.value;

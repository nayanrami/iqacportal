              programSelect.innerHTML = '<option value="">Select Program</option>';
            }
          } else {
            programSelect.innerHTML = '<option value="">Select Program</option>';
          }
        });
      }

      console.log("Form initialization complete");
    } catch (error) {
      console.error("Error during form initialization:", error);
    }
  }


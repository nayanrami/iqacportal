  // Add email validation function
  function validateEmail(email) {
    const re =
      /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(String(email).toLowerCase());
  }

  // Add email handler
  function handleEmailValidation() {
    const emailInput = document.getElementById("email");
    const emailError = document.getElementById("emailError");

    emailInput.addEventListener("blur", function () {
      const email = this.value.trim();

      if (email === "") {
        emailError.textContent = "Email is required";
        emailError.style.display = "block";
        this.classList.add("error");
        this.classList.remove("valid");
        return;
      }

      if (!validateEmail(email)) {
        emailError.textContent = "Please enter a valid email address";
        emailError.style.display = "block";
        this.classList.add("error");
        this.classList.remove("valid");
        return;
      }

      // Check if email domain exists (optional)
      const domain = email.split("@")[1];
      if (!domain.includes(".")) {
        emailError.textContent = "Invalid email domain";
        emailError.style.display = "block";
        this.classList.add("error");
        this.classList.remove("valid");
        return;
      }

      // Email is valid
      emailError.style.display = "none";
      this.classList.remove("error");
      this.classList.add("valid");
    });

    // Add input event listener to clear error on typing
    emailInput.addEventListener("input", function () {
      if (this.value.trim() === "") {
        emailError.style.display = "none";
        this.classList.remove("error", "valid");
      }
    });
  }

  // Update form submission to include email validation
  async function submitApplication(event) {
    event.preventDefault();
    console.log("Form submission started");

    const emailInput = document.getElementById("email");
    const email = emailInput.value.trim();
    const emailError = document.getElementById("emailError");

    const isValidEmail = /^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email);
    if (!isValidEmail) {
      emailError.textContent = "Please enter a valid email address";
      emailError.style.display = "block";
      emailInput.classList.add("error");
      emailInput.focus();
      return;
    }

    const formData = new FormData(event.target);
    const formObject = Object.fromEntries(formData.entries());
    console.log("Form data collected:", formObject);

    const fullName = formObject.name.trim();
    const [first_name, ...rest] = fullName.split(" ");
    const last_name = rest.join(" ") || "";

    const stateId = parseInt(formObject.state);
    const cityId = parseInt(formObject.city);
    const programId = parseInt(formObject.program);
    const eligibilityId = parseInt(formObject.eligibility);

    // Get program select element and selected option
    const programSelect = document.getElementById("program");
    const selectedOption = programSelect.options[programSelect.selectedIndex];
    console.log("Selected program:", selectedOption.value);

    // Get phone number
    const phoneInput = document.getElementById("phone");
    const phoneNumber = phoneInput ? phoneInput.value.trim() : "";

    try {
      // First fetch program details to get admission_id
      const programResponse = await fetch(`https://api.cvmu.edu.in/applicant/api/v1/applicant_portal/get_programs?status=Open&eligibility_head_id=${eligibilityId}`);
      const programData = await programResponse.json();

      if (!programData.success) {
        throw new Error("Failed to fetch program details");
      }

      // Find the selected program
      const selectedProgram = programData.data.results.find(p => p.program_id === programId);
      if (!selectedProgram) {
        throw new Error("Selected program not found");
      }

      console.log("Selected program details:", selectedProgram);

      // Create the payload with correct admission_id
      const payload = {
        first_name,
        last_name,
        full_name: fullName,
        mobile_number: phoneNumber ? `+91-${phoneNumber}` : "",
        email: formObject.email,
        city_id: cityId,
        district_id: cityId,
        state_id: stateId,
        faculty_id: null,
        phd_subject_id: null,
        program_type_id: selectedProgram.program_data.program_type_id,
        phd_admission_process_id: null,
        eligibility_head_id: eligibilityId,
        program_data: [{
          admission_id: selectedProgram.admission_id,
          program_id: programId,
          is_approved: true
        }],
        is_mobile_number_verified: true,
        utm_source: "cvmu.edu.in",
        utm_medium: "portal",
        utm_campaign: "cvmu_website",
        utm_term: "all",
        utm_content: "website"
      };

      console.log("Submitting payload:", payload);

      const response = await fetch("https://api.cvmu.edu.in/applicant/api/v1/applicant_user_register/add", {
        method: "POST",
        headers: {
          "Content-Type": "application/json"
        },
        body: JSON.stringify(payload)
      });

      const data = await response.json();
      console.log(" Registration API Response:", data);

      if (data.success) {
        // Close the form first
        closeApplyForm();

        // Create and show success message
        const message = document.createElement("div");
        message.style.position = "fixed";
        message.style.top = "50%";
        message.style.left = "50%";
        message.style.transform = "translate(-50%, -50%)";

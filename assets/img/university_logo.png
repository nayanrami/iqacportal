  // Function to populate state dropdown
  async function populateStateDropdown() {
    const states = await fetchStates();
    const stateSelects = document.querySelectorAll('select[name="state"]');
    let gujaratOption;

    stateSelects.forEach((select) => {
      select.innerHTML = '<option value="">Select State *</option>';

      // Sort states alphabetically
      states.sort((a, b) => a.name.localeCompare(b.name));

      // Add Gujarat as first option
      gujaratOption = states.find((state) => state.name === "Gujarat");
      if (gujaratOption) {
        const option = document.createElement("option");
        option.value = gujaratOption.id;
        option.textContent = gujaratOption.name;
        option.selected = true;
        select.appendChild(option);
      }

      // Add other states
      states.forEach((state) => {
        if (state.name !== "Gujarat") {
          const option = document.createElement("option");
          option.value = state.id;
          option.textContent = state.name;
          select.appendChild(option);
        }
      });
    });

    // Immediately populate cities for Gujarat
    if (gujaratOption) {
      await populateCityDropdown(gujaratOption.id);
    }
  }

  // Function to populate city dropdown

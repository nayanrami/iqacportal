  // Update the initializeSectionBehavior function
  function initializeSectionBehavior() {
    const banner = document.querySelector(".section1_banner_swiper");

    const observer = new IntersectionObserver(
      (entries) => {
        entries.forEach((entry) => {
          if (entry.isIntersecting) {

  // Add this to your existing JavaScript
  function initializeOTPInputs() {
    const otpWrapper = document.querySelector(".otp-wrapper");
    const otpInputs = otpWrapper.querySelectorAll(".otp-input");
    const hiddenOtpInput = document.getElementById("otp");
    const resendWrapper = document.querySelector(".resend-otp-wrapper");
    let isVerifying = false; // Add flag to prevent duplicate verification

    // Initially disable OTP inputs
    otpInputs.forEach((input) => {
      input.disabled = true;
    });

    // Function to enable OTP inputs and show timer
    window.enableOTPInput = function () {
      otpInputs.forEach((input) => {
        input.disabled = false;
        input.value = "";
      });
      otpInputs[0].focus();
      resendWrapper.style.display = "flex";
      startOtpTimer();
    };

    // Allow only numbers
    function isNumberKey(evt) {
      const charCode = evt.which ? evt.which : evt.keyCode;
      if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        evt.preventDefault();
        return false;
      }
      return true;
    }

    // Update hidden input with complete OTP
    function updateHiddenInput() {
      const otp = Array.from(otpInputs)
        .map((input) => input.value)
        .join("");
      hiddenOtpInput.value = otp;

      if (otp.length === 6 && !window.isVerifyingOTP) {
        window.isVerifyingOTP = true;
        verifyOTP().finally(() => {
          window.isVerifyingOTP = false;
        });
      }
    }


    otpInputs.forEach((input, index) => {
      input.addEventListener("keydown", isNumberKey);

      input.addEventListener("input", (e) => {
        const currentInput = e.target;
        const nextInput = otpInputs[index + 1];
        currentInput.value = currentInput.value.replace(/[^0-9]/g, "");

        if (currentInput.value !== "" && nextInput) {
          nextInput.focus();
        }
        updateHiddenInput();
      });

      input.addEventListener("keydown", (e) => {
        const currentInput = e.target;
        const prevInput = otpInputs[index - 1];

        if (e.key === "Backspace") {
          if (currentInput.value === "" && prevInput) {
            e.preventDefault();
            prevInput.value = "";
            prevInput.focus();
            updateHiddenInput();
          }
        }
      });

      input.addEventListener("paste", (e) => {
        e.preventDefault();
        const pastedData = e.clipboardData.getData("text").slice(0, 6);
        const numbers = pastedData.replace(/[^0-9]/g, "").split("");

        otpInputs.forEach((input, i) => {
          input.value = numbers[i] || "";
        });

        if (numbers.length > 0) {
          otpInputs[Math.min(numbers.length, 5)].focus();
        }
        updateHiddenInput();
      });
    });
  }


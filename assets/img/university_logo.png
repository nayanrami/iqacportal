<script>
  // Add these functions at the beginning of your script section
  async function fetchCountryCodes() {
    try {
      const response = await fetch(
        "https://api.cvmu.edu.in/hrms/api/v1/country/view?limit=300&offset=0"
      );
      const data = await response.json();
      return data.data.results;
    } catch (error) {
      console.error("Error fetching country codes:", error);
      return [];
    }
  }

  function populateCountryCodeDropdown(countries) {
    const countryCodeSelects = document.querySelectorAll(".country_code");

    countryCodeSelects.forEach((select) => {
      select.innerHTML = "";

      // Sort countries by name
      countries.sort((a, b) => a.name.localeCompare(b.name));

      // Add India as first option
      const indiaOption = countries.find((country) => country.name === "India");
      if (indiaOption) {
        const option = document.createElement("option");
        option.value = indiaOption.code;
        option.textContent = `+${indiaOption.code.replace("+", "")}`;
        option.selected = true;
        select.appendChild(option);
      }

      // Add other countries
      countries.forEach((country) => {
        if (country.name !== "India") {
          const option = document.createElement("option");
          option.value = country.code;
          option.textContent = `+${country.code.replace("+", "")}`;
          select.appendChild(option);
        }
      });
    });
  }

  // Add these functions at the beginning of your script section
  async function fetchStates() {
    try {
      const response = await fetch(
